{
  "name": "aou-puppeteer-test-automation",
  "description": "All-of-Us test automation framework",
  "version": "0.0.1",
  "main": "index.js",
  "private": true,
  "license": "BSD",
  "scripts": {
    "test": "cross-env jest --maxWorkers=5",
    "test:debugTest": "cross-env PUPPETEER_HEADLESS=false DEBUG=true node --inspect-brk node_modules/.bin/jest",
    "test:debug": "cross-env PUPPETEER_HEADLESS=false DEBUG=true jest --detectOpenHandles",
    "test-local": "cross-env WORKBENCH_ENV=local DEBUG=true jest --maxWorkers=3",
    "test-local:debug": "cross-env WORKBENCH_ENV=local PUPPETEER_HEADLESS=false DEBUG=true jest --maxWorkers=3",
    "test-staging": "cross-env WORKBENCH_ENV=staging jest --maxWorkers=3",
    "test-staging:debug": "cross-env WORKBENCH_ENV=staging PUPPETEER_HEADLESS=false DEBUG=true jest --maxWorkers=1",
    "test:ci": "jest --no-color --ci --maxWorkers=1 --detectOpenHandles",
    "test:ci:debug": "jest --ci --debug --maxWorkers=1 --bail 1",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "yarn compile && yarn run lint --fix",
    "tsc-build": "tsc --build --clean && tsc --skipLibCheck --project tsconfig.json",
    "compile": "rimraf tsc-out/ && yarn run tsc-build"
  },
  "keywords": [
    "test",
    "puppeteer",
    "headless",
    "chrome",
    "ui"
  ],
  "engines": {
    "node": ">= 10.0.0"
  },
  "jest-junit": {
    "outputDirectory": "./logs",
    "outputName": "test-results.xml",
    "classNameTemplate": "{filepath}",
    "suiteNameTemplate": "{filepath}"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/expect-puppeteer": "4.4.5",
    "@types/faker": "5.1.5",
    "@types/jest": "26.0.16",
    "@types/jest-environment-puppeteer": "4.4.0",
    "@types/lodash": "4.14.165",
    "@types/node": "14.14.10",
    "@types/puppeteer": "5.4.1",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "faker": "5.1.0",
    "fs-extra": "9.0.1",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "jest-config": "26.6.3",
    "jest-junit": "^11.0.1",
    "jest-puppeteer": "4.4.0",
    "jest-stare": "2.2.0",
    "lodash": "4.17.20",
    "puppeteer": "5.5.0",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.0",
    "tslib": "2.0.3",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-microsoft-contrib": "6.2.0",
    "typescript": "4.1.2"
  }
}
