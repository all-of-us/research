{
  "name": "aou-puppeteer-test-automation",
  "description": "All-of-Us test automation framework",
  "version": "0.0.1",
  "main": "index.js",
  "private": true,
  "license": "BSD",
  "scripts": {
    "preinstall": "$npm_execpath --silent run checkyarn",
    "checkyarn": "echo \"$npm_execpath\" | grep -q \"yarn\\.js$\" || (echo '⚠️  Use yarn not npm!️ ' && echo && exit 1)",
    "test:ci": "cross-env NODE_ENV=development WORKBENCH_ENV=dev INCOGNITO=true jest --maxWorkers=3 --detectOpenHandles --forceExit",
    "test": "cross-env WORKBENCH_ENV=dev INCOGNITO=true jest --detectOpenHandles --forceExit",
    "test:headful": "cross-env WORKBENCH_ENV=dev HEADLESS=false INCOGNITO=true jest --detectOpenHandles --runInBand --forceExit",
    "test-local": "cross-env WORKBENCH_ENV=local jest",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "yarn run lint --fix",
    "build": "tsc --build --clean",
    "compile": "rimraf tsc-out/ && yarn run lint && yarn run build"
  },
  "jest": {
    "verbose": true,
    "preset": "jest-puppeteer",
    "roots": [
      "<rootDir>/tests"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "userAgent": "Mozilla/5.0 (Macintosh; Intel Mac OS X 10_14_6) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/78.0.3904.87 Safari/537.36"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "<rootDir>/**/tests/**/*.spec.ts"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!tests)"
    ],
    "moduleFileExtensions": [
      "ts",
      "spec.ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [
    "test",
    "puppeteer",
    "headless",
    "chrome",
    "ui"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/expect-puppeteer": "^4.4.0",
    "@types/faker": "^4.1.10",
    "@types/jest": "^25.1.3",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/node": "^13.7.7",
    "@types/puppeteer": "^2.0.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.1.0",
    "faker": "^4.1.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "jest-puppeteer": "^4.4.0",
    "lodash": "^4.17.15",
    "puppeteer": "^2.1.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.4.1",
    "tslib": "^1.11.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.8.3"
  }
}
