{
  "name": "aou-puppeteer-test-automation",
  "description": "All-of-Us test automation framework",
  "version": "0.0.1",
  "main": "index.js",
  "private": true,
  "license": "BSD",
  "scripts": {
    "test": "cross-env WORKBENCH_ENV=dev INCOGNITO=true jest --detectOpenHandles",
    "test:debug": "cross-env WORKBENCH_ENV=dev HEADLESS=false INCOGNITO=true jest --detectOpenHandles --maxWorkers=2",
    "test-local": "cross-env WORKBENCH_ENV=local INCOGNITO=true jest",
    "test:ci": "cross-env NODE_ENV=development WORKBENCH_ENV=dev INCOGNITO=true CI=true jest --ci --runInBand --forceExit",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "yarn run lint --fix",
    "tsc-build": "tsc --skipLibCheck --project tsconfig.json",
    "compile": "rimraf tsc-out/ && yarn run tsc-build"
  },
  "jest": {
    "verbose": true,
    "preset": "jest-puppeteer",
    "testTimeout": 240000,
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json"
      }
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "<rootDir>/**/tests/**/*.spec.ts"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!tests)"
    ],
    "moduleFileExtensions": [
      "ts",
      "spec.ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [
    "test",
    "puppeteer",
    "headless",
    "chrome",
    "ui"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/expect-puppeteer": "^4.4.0",
    "@types/faker": "^4.1.10",
    "@types/jest": "^25.1.3",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.7",
    "@types/puppeteer": "^2.0.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.1.0",
    "faker": "^4.1.0",
    "fs-extra": "^9.0.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "jest-puppeteer": "^4.4.0",
    "lodash": "^4.17.15",
    "puppeteer": "^2.1.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.4.1",
    "tslib": "^1.11.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.8.3"
  }
}
