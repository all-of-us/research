<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">
  <changeSet author="brianfreeman" id="changelog-1">

    <createTable tableName="icd9_criteria">
      <column name="id" type="bigint" autoIncrement="true">
        <constraints primaryKey="true" nullable="false"/>
      </column>
      <column name="parent_id" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="type" type="varchar(20)">
        <constraints nullable="false"/>
      </column>
      <column name="code" type="varchar(80)">
        <constraints nullable="false"/>
      </column>
      <column name="name" type="varchar(80)">
        <constraints nullable="true"/>
      </column>
      <column name="is_group" type="boolean">
        <constraints nullable="true"/>
      </column>
      <column name="is_selectable" type="boolean">
        <constraints nullable="true"/>
      </column>
      <column name="count" type="bigint">
        <constraints nullable="true"/>
      </column>
      <column name="domain_id" type="varchar(80)">
        <constraints nullable="true"/>
      </column>
      <column name="concept_id" type="bigint">
        <constraints nullable="true"/>
      </column>
    </createTable>

  </changeSet>
</databaseChangeLog>
