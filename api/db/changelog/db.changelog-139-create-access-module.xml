<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">
  <changeSet author="jaycarlton" id="changelog-139-create-access-module">
    <createTable tableName="access_module">
      <column name="access_module_id" autoIncrement="true" type="bigint">
        <constraints primaryKey="true" nullable="false"/>
      </column>
      <column name="display_name" type="varchar(100)">
        <constraints nullable="false"/>
      </column>
        <column name="evaluator_key" type="tinyint">
          <constraints nullable="false"/>
        </column>
        <column name="module_type" type="tinyint">
          <constraints nullable="false"/>
        </column>
    </createTable>
  </changeSet>
</databaseChangeLog>
