<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <service>api</service>
  <runtime>java8</runtime>
  <threadsafe>true</threadsafe>

  <env-variables>
    <env-var name="spring.datasource.driver-class-name" value="com.mysql.jdbc.Driver"/>
    <env-var name="spring.datasource.url" value="jdbc:mysql://localhost:3306/workbench"/>
    <env-var name="spring.datasource.username" value="workbench"/>
    <env-var name="spring.datasource.password" value="workbench!pwd"/>
  </env-variables>
</appengine-web-app>
