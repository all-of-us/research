{
  "name": "aou-ui-angular",
  "version": "0.0.2",
  "license": "BSD",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --sourcemaps=false",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "codegen": "yarn run codegen-clean && yarn run codegen-swagger && yarn run post-codegen",
    "codegen-clean": "rm -rf src/generated",
    "codegen-swagger": "java -jar ../libproject/swagger-codegen-cli.jar generate --lang typescript-angular --input-spec ../api/src/main/resources/workbench.yaml --output src/generated --additional-properties ngVersion=2",
    "post-codegen": "sed -i.bak -e 's/OpaqueToken/InjectionToken/' src/generated/variables.ts"
  },
  "private": true,
  "dependencies": {
    "@angular-redux/store": "^6.6.0",
    "@angular/animations": "^5.2.2",
    "@angular/common": "^5.2.2",
    "@angular/compiler": "^5.2.2",
    "@angular/core": "^5.2.2",
    "@angular/forms": "^5.2.2",
    "@angular/http": "^5.2.2",
    "@angular/platform-browser": "^5.2.2",
    "@angular/platform-browser-dynamic": "^5.2.2",
    "@angular/router": "^5.2.2",
    "@clr/angular": "^0.11.2-patch",
    "@clr/icons": "^0.11.2-patch",
    "@clr/ui": "^0.11.2-patch",
    "@types/geojson": "^1.0.6",
    "@webcomponents/custom-elements": "^1.0.0",
    "angular-2-local-storage": "^1.0.1",
    "angular2-highcharts": "^0.5.5",
    "bootstrap": "^4.0.0-alpha.5",
    "core-js": "^2.5.3",
    "d3": "^4.12.2",
    "dom-to-image": "^2.6.0",
    "enhanced-resolve": "3.3.0",
    "highcharts": "^6.0.4",
    "immutable": "^4.0.0-rc.9",
    "moment": "^2.20.1",
    "mutationobserver-shim": "^0.3.2",
    "ngx-popper": "^1.9.9",
    "popper.js": "^1.13.0",
    "redux": "^3.7.2",
    "redux-observable": "^0.16.0",
    "rxjs": "^5.5.6",
    "stackdriver-errors-js": "^0.4.0",
    "stacktrace-js": "^2.0.0",
    "zone.js": "0.8.12"
