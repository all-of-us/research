{
  "name": "aou-ui-angular",
  "version": "0.0.2",
  "license": "BSD",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev-up": "yarn && yarn run codegen && yarn start",
    "build": "ng build",
    "test": "ng test --source-map=false",
    "test-react": "jest",
    "lint": "yarn eslint src --ext ts,tsx && ng lint --type-check",
    "e2e": "ng e2e",
    "codegen": "yarn run codegen-clean && yarn run codegen-swagger && yarn run codegen-swagger-fetch && yarn run post-codegen && yarn run notebooks-codegen-swagger-fetch",
    "codegen-clean": "rm -rf src/generated src/notebooks-generated",
    "codegen-swagger": "java -jar ../aou-utils/swagger-codegen-cli.jar generate --lang typescript-angular --input-spec ../api/src/main/resources/workbench-api.yaml --output src/generated --additional-properties ngVersion=2",
    "codegen-swagger-fetch": "java -jar ../aou-utils/swagger-codegen-cli.jar generate --lang typescript-fetch --input-spec ../api/src/main/resources/workbench-api.yaml --output src/generated/fetch",
    "post-codegen": "sed -i.bak -e 's/OpaqueToken/InjectionToken/' src/generated/variables.ts",
    "notebooks-codegen-swagger-fetch": "java -jar ../aou-utils/swagger-codegen-cli.jar generate --lang typescript-fetch --input-spec ../api/src/main/resources/notebooks.yaml --output src/notebooks-generated/fetch",
    "coverage": "ng test --source-map=false --watch=false --code-coverage"
  },
  "jest": {
    "clearMocks": true,
    "setupFiles": [
      "./test-shim.js",
      "./test-setup.js"
    ],
    "setupTestFrameworkScriptFile": "./test-setup-script.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "spec.ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "src/tsconfig.jest.json"
      }
    }
  },
  "browserslist": [
    "chrome >= 67"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.2",
    "@angular/common": "^5.2.2",
    "@angular/compiler": "7.1.3",
    "@angular/core": "^5.2.2",
    "@angular/forms": "^5.2.2",
    "@angular/http": "^5.2.2",
